{
  "name": "fetch-multi-source",
  "version": "1.0.0",
  "description": "This is a library allowing to fetch a resource from multiple sources",
  "main": "dist/main.js",
  "scripts": {
    "doInstall": "npm install",
    "test-not-working": "mocha --require ts-node/register src/**/*.test.ts",
    "spec": "npm run wdio",
    "wdio": "wdio run ./wdio.conf.ts",
    "wdio-watch": "wdio run ./wdio.conf.ts --watch",
    "start": "parcel --no-cache",
    "prebuild": "rimraf dist",
    "build": "parcel build --no-cache",
    "local": "npm run build && npm link",
    "prepack": "npm run build",
    "pack": "npm pack"
  },
  "author": "Isirode",
  "license": "SEE LICENSE IN COPYING",
  "source": "src/index.ts",
  "types": "dist/types.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.map",
    "src/"
  ],
  "devDependencies": {
    "@parcel/packager-ts": "2.9.2",
    "@parcel/transformer-typescript-tsc": "2.9.2",
    "@parcel/transformer-typescript-types": "2.9.2",
    "@types/chai": "4.3.5",
    "@types/mocha": "10.0.1",
    "@types/node": "20.1.4",
    "@types/parse-torrent": "5.8.4",
    "@wdio/browser-runner": "8.10.7",
    "@wdio/cli": "8.10.7",
    "@wdio/globals": "8.10.7",
    "@wdio/mocha-framework": "8.10.7",
    "chai": "4.3.7",
    "events": "3.3.0",
    "fetch-progress": "https://github.com/isirode/fetch-progress/releases/download/1.1.0/fetch-progress-1.1.0.tgz",
    "mocha": "10.2.0",
    "parcel": "2.9.2",
    "parse-torrent": "11.0.12",
    "path-browserify": "1.0.1",
    "rimraf": "4.1.3",
    "ts-jest": "29.0.5",
    "ts-mockito": "2.6.1",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "buffer": "6.0.3",
    "emittery": "1.0.1",
    "log4j2-typescript": "https://github.com/isirode/log4j2-typescript/releases/download/1.0.2/log4j2-typescript-1.0.2.tgz",
    "p-wait-for": "5.0.2"
  }
}
